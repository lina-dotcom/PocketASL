<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.level11.alphabet.q32"
    android:background="#f7ebc3">


    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/t"
        android:layout_marginLeft="80dp" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/r"
        android:layout_marginLeft="80dp" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/o"
        android:layout_marginLeft="80dp" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="190dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/p"
        android:layout_marginLeft="80dp" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="260dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/h"
        android:layout_marginLeft="80dp" />

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginStart="330dp"
        android:layout_marginTop="330dp"
        android:background="@drawable/y"
        android:layout_marginLeft="80dp" />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="175dp"
        android:layout_marginTop="140dp"
        android:text="What's this word?"
        android:textAlignment="center"
        android:textSize="50dp"
        android:gravity="center_horizontal" />

    <Button
        android:id="@+id/q32answer"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:text="Answer!"
        android:textAlignment="center"
        android:textSize="30sp"
        android:layout_marginStart="80dp"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="520dp"
        android:gravity="center_horizontal"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="10dp"
        android:backgroundTint="#f0be1a"/>


</FrameLayout>